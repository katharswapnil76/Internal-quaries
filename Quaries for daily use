SELECT 'ALTER INDEX '||INDEX_OWNER||'.'||INDEX_NAME||'REBUILD PARTITION '||PARTITION_NAME||' PARALLEL 8 ONLINE;' FROM DBA_IND_PARTITIONS WHERE STATUS='UNUSABLE';

