select * from dba_users where username='PERFSTAT';
SELECT SNAP_ID, SNAP_TIME FROM STATS$SNAPSHOT ORDER BY 1;
select OBJECT_NAME,OBJECT_TYPE from dba_objects where owner='PERFSTAT';
SELECT rdsadmin.rdsadmin_adrci_util.list_adrci_incidents AS task_id FROM DUAL;
SELECT rdsadmin.rdsadmin_adrci_util.show_adrci_tracefile AS task_id FROM DUAL;
VAR task_id VARCHAR2(80);
EXEC :task_id := rdsadmin.rdsadmin_adrci_util.show_adrci_tracefile;
SELECT * FROM TABLE(rdsadmin.rds_file_util.read_text_file('BDUMP', 'dbtask-'||:task_id||'.log')) WHERE TEXT LIKE '%/alert_%';
SELECT * FROM TABLE(rdsadmin.rds_file_util.read_text_file('BDUMP', 'alert_OLAP.log.2024-06-24')) ;
     
select * from v$version;
